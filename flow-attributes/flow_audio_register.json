{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "description": "Describes Audio Flow additional and extensible attributes defined in the NMOS Parameter Registers.",
  "title": "Video Flow resource",
  "allOf": [
    { "$ref": "flow_audio_base_register.json" },
    {
      "anyOf": [
        { "not": { "properties": { "media_type": { "enum": [ "audio/mpeg4-generic" ] } } } },
        { "$ref": "flow_audio_aac_register.json" }
      ]
    },
    {
      "anyOf": [
        { "not": { "properties": { "media_type": { "enum": [ "audio/MP4A-LATM" ] } } } },
        { "$ref": "flow_audio_aac_register.json" }
      ]
    },
    {
      "anyOf": [
        { "not": { "properties": { "media_type": { "enum": [ "audio/MP4A-ADTS" ] } } } },
        { "$ref": "flow_audio_aac_register.json" }
      ]
    }
  ]
}
